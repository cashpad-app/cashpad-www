.graph(paths-pie="pie")
  svg(ng-if="!!viewport", ng-attr-width="{{viewport.width}}",
    ng-attr-height="{{viewport.height}}", 
    ng-attr-transform="translate({{viewport.width}}, {{viewport.height}})")
      path(ng-repeat="curve in curves", 
        ng-attr-d="{{curve._sector}}",
        stroke="white")
